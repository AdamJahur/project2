<div class="row">
    <div class="col-lg-12">
        <div class="m-b-20">
            <h5>List of Veterans</h5>

            <table class="table m-0">
                
                <thead>
                    <tr>
                        <th>#</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Rank</th>
                    </tr>
                </thead>
                <tbody>
                {{#each employers}}
                    <tr>
                        <th scope="row">{{this.id}}</th>
                        <td><a href="/employer/{{this.id}}">{{this.firstName}}</a></td>
                        <td>{{this.lastName}}</td>
                        <td>{{this.rank}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>

                      





















